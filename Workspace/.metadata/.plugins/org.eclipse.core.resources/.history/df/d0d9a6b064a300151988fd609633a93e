package utilities;

public enum Offices
{
	USP (new String[] {"PRESIDENT", "PRESIDENTIAL"}, new String[] {}),
	USS (new String[] {"SENATE", "SENATOR"}, new String[] {"STATE ", "DISTRICT "}),
	USH (new String[] {"CONGRESS", "HOUSE", "REPRESENTATIVE"}, new String[] {}),
	GOV (new String[] {"GOVERNOR", "GUBERNATORIAL", "GOV"}, new String[] {"LIEUTENANT", "LT"}),
	LTG (new String[] {"LIEUTENANT"}, new String[] {}),
	STS (new String[] {"STATE SENATE", "STATE SENATOR"}, new String[] {"US", "U.S", "UNITED STATES"}),
	STH (new String[] {"STATE HOUSE", "STATE REPRESENTATIVE", "ASSEMBLY"},
			new String[] {"US", "U.S", "UNITED STATES"}),
	SOS (new String[] {"ATTORNEY GENERAL"}, new String[] {}),
	ATG (new String[] {"SECRETARY OF STATE", "SECY OF STATE", "SEC. OF STATE"},
			new String[] {}),
	TRE (new String[] {"TREASURER"}, new String[] {});
	
	
	private String[] keys, exclusions;
	
	private Offices(String[] _keys, String[] _exclusions)
	{
		this.keys = _keys;
		this.exclusions = _exclusions;
	}
	
	public String[] getKeys()
	{
		return this.keys;
	}
	
	public String[] getExclusions()
	{
		return this.exclusions;
	}
}

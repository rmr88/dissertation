package tests;

import tasks.RunShell;

public class ShellLaunch
{
	public static void main(String args[])
	{
		String command = "rsync -avz --delete govtrack.us::govtrackdata/us/#/@/ #";
		String dir = "C:\\Users\\Robbie\\Documents\\dissertation\\Data\\GovTrack";
		
		for (int cong = 93; cong <= 113; cong++)
		{
			RunShell shell = new RunShell(command.replaceAll("#", Integer.toString(cong))
					.replace("@", "bills"), dir);
			shell.run();
			
			shell = new RunShell(command.replaceAll("#", Integer.toString(cong))
					.replace("@", "rolls"), dir);
			shell.run();
			
			System.out.println(cong + " read to disk.");
		}
	}

}

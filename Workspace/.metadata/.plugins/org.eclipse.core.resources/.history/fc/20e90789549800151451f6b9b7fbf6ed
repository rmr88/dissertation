package utilities;

public class DataObj
{
	private String colName;
	private int colNum;
	private String[] data;
	
	public DataObj() {}
	
	public DataObj(String name, int num)
	{
		this.colName = name;
		this.colNum = num;
	}
	
	public String getName()
	{
		return this.colName;
	}
	
	public int getNum()
	{
		return this.getNum();
	}
	
	public String[] getData()
	{
		return this.data;
	}
	
	public void addData(String cell)
	{
		this.data[this.data.length] = cell;
	}
}

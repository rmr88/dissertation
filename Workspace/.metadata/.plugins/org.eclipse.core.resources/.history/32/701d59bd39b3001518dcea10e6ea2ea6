package utilities;

public enum Columns
{
	LOCATION (new String[] {"county", "precinct"}),
	OFFICE (new String[] {"office"}),
	PARTY (new String[] {"party"}),
	VOTE (new String[] {"votes", "vote", "total"}),
	CANDNAME (new String[] {"candidate", "cand", "can"});
	
	private String[] matchArray;
	private Columns(String[] matches)
	{
		this.matchArray = matches;
	}
	
	public boolean check(String toCheck)
	{
		for (String checkAgainst : this.matchArray)
			if (toCheck.contains(checkAgainst))
				return true;
				
		return false; //if no match found
	}
}
